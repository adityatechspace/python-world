name="John" #String
age=22 #integer
cgpa= 8.30 #float
is_pass=True #boolean

print(type(name))
print(type(age))
print(type(cgpa))
print(type(is_pass))


#Conversion of data types
num1="5"
num2="6"

print(num1+num2) #Concatenation of strings

num1=int(num1) #Conversion of string to integer
num2=int(num2)

print(num1+num2) #Addition of integers

#Multiple Assignment
a, b, c = 5, 7.5, "Hello"

print(a,b,c)

#Taking user input
student=input("Enter your name:")
marks=float(input("Enter your marks:"))
print("Student Name:", student)
print("makrs obtained:", marks)